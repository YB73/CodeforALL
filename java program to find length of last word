class Solution {
    public int lengthOfLastWord(String s) {
        
       String str=s.strip();
        int f=0;
        for(int i=str.length()-1;i>=0;i--)
        {
            char c=str.charAt(i);
            if(c!=' ')
                f++;
            else 
                break;
        }
        return f;
                
    }
}
